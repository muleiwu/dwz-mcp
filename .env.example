# 远程短网址服务器配置
REMOTE_BASE_URL=https://api.short.example.com
REMOTE_API_KEY=your-remote-api-key-here

# API 版本
API_VERSION=v1

# 请求超时时间（毫秒）
REQUEST_TIMEOUT=10000

# 重试次数
MAX_RETRIES=3

# 重试间隔（毫秒）
RETRY_DELAY=1000

# MCP 服务器配置
MCP_SERVER_NAME=mliev-dwz-client
MCP_SERVER_VERSION=1.0.0

# 日志级别
LOG_LEVEL=info